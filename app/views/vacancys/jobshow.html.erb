<body>
  <table class="table  table-success table-striped">
    <thead class="table-dark">
      </tr>
        <th>Name</th>
        <th>Number</th>
        <th>Email</th>
        <th colspan= "3">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @vs.each do |seeker| %>
    
        <tr>
          <td><%= seeker.name %></td>
          <td><%= seeker.number %></td>
          <td><%= seeker.email %></td>
          <td><button><%= link_to 'Show', job_seeker_path(seeker) %></button>
          <td><button><%= link_to 'Delete', job_seeker_path(seeker),data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %></button></td>
          <td><button><%= link_to 'Edit', edit_job_seeker_path(seeker) %></button></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</body>