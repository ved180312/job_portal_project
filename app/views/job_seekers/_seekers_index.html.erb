<body>
  <table class="table ">
    <thead>
      </tr>
        <th>Name</th>
        <th>Number</th>
        <th>Email</th>
        <th colspan= "3">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @js.each do |seeker| %>

        <tr>
          <td><%= seeker.name %></td>
          <td><%= seeker.number %></td>
          <td><%= seeker.email %></td>
          <td><%= link_to 'Show', job_seeker_path(seeker), class: "btn btn-primary" %></td>
          <td><%= link_to 'Delete', job_seeker_path(seeker),data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}, class: "btn btn-primary" %></td>
          <td><%= link_to 'Edit', edit_job_seeker_path(seeker), class: "btn btn-primary" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</body>
